{
	"name": "CopyFromOnPremSqlToAzureBlobPipeline",
	"properties": {
		"activities": [
			{
				"name": "SQLToBlob",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "dbo.emp"
					},
					{
						"name": "Destination",
						"value": "adfstorage//"
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource"
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"quoteAllText": true
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "ID",
									"type": "Int32"
								},
								"sink": {
									"path": "$['ID']"
								}
							},
							{
								"source": {
									"name": "FirstName",
									"type": "String"
								},
								"sink": {
									"path": "$['FirstName']"
								}
							},
							{
								"source": {
									"name": "LastName",
									"type": "String"
								},
								"sink": {
									"path": "$['LastName']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDSSQLToBlob",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SinkDSSQLToBlob",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}